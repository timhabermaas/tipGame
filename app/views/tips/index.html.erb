<table class="tips">
  <thead>
    <th>users</th>
    <% @matches.each do |match| %>
      <th><%= short_match match %></th>
    <% end %>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <th><%= user.name %></th>
        <% @matches.each do |match| %>
          <% tip = @tips.find { |t| t.user == user && t.match == match } %>
          <td><%= tip.try(:score) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
